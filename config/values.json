{
  "environment": "TEST",
  "apiVersion": "machinelearning.seldon.io/v1",
  "deploymentName": "graph-metadata-router",
  "namespace": "default",
  "chart": {
    "name": "test-model",
    "version": "0.1.0",
    "description": "Test Seldon deployment chart"
  },
  "subjectArea": "seldon-test-project",
  "sourceMetafileName": "seldon.meta.yaml",
  "sourceMetafileRepo": "https://neogit.neoflex.ru/vsemenov/tservice/",
  "sourceMetafileBranch": "main",
  "predictors": [
    {
      "name": "example",
      "replicas": 1,
      "componentSpec": {
        "containers": [
          {
            "name": "node-router",
            "image": "seldonio/metadata-generic-node:0.4"
          },
          {
            "name": "node-one",
            "image": "seldonio/metadata-generic-node:0.4",
            "env": [
              {
                "name": "MODEL_METADATA",
                "value": {
                  "name": "node-one",
                  "versions": "[ generic-node/v0.4 ]",
                  "platform": "seldon",
                  "inputs": {
                    "messagetype": "tensor",
                    "schema": {
                      "names": "[ a, b ]",
                      "shape": "[ 2 ]"
                    }
                  },
                  "outputs": {
                    "messagetype": "tensor",
                    "schema": {
                      "names": "[ node-output ]",
                      "shape": "[ 1 ]"
                    }
                  }
                }
              }
            ]
          },
          {
            "name": "node-two",
            "image": "seldonio/metadata-generic-node:0.4",
            "env": [
              {
                "name": "MODEL_METADATA",
                "value": {

                  "name": "node-two",
                  "versions": "[ generic-node/v0.4 ]",
                  "platform": "seldon",
                  "inputs": {
                  "messagetype": "tensor",
                    "schema": {
                      "names": "[ a, b ]",
                      "shape": "[ 2 ]"
                    }
                  },
                  "outputs": {
                    "messagetype": "tensor",
                    "schema": {
                      "names": "[ node-output ]",
                      "shape": "[ 1 ]"
                    }
                  }
                }
              }
            ]
          }
        ]
      },
      "graph": {
        "name": "node-router",
        "type": "ROUTER",
        "children": [
          {
            "name": "node-one",
            "type": "MODEL",
            "children": []
          },
          {
            "name": "node-two",
            "type": "MODEL",
            "children": []
          }
        ]
      }
    }
  ]
}
