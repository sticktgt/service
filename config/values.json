{
  "Environment": "TEST",
  "Chart": {
    "Name": "fs33kafka-chart",
    "Version": "0.1.0",
    "Description": "Loan approval model"
  },
  "DeploymentName": "fs33kafka",
  "Namespace": "app-fdd",
  "Predictors": [
    {
      "Name": "loan-approval",
      "Replicas": 1,
      "Traffic": 100,
      "SvcOrchEnv": [
        { "Name": "LOGGER_KAFKA_BROKER", "Value": "kafka.example.com:9093" },
        {
          "Name": "KAFKA_SSL_CA_CERT",
          "ValueFrom": {
            "SecretKeyRef": {
              "Name": "kafka-creds",
              "Key": "ca.cert"
            }
          }
        }
      ],
      "ComponentSpec": {
        "ServiceAccountName": "default-editor",
        "TerminationGracePeriodSeconds": 0,
        "Containers": [
          {
            "Name": "fs33kafka",
            "Image": "my-registry/fs33:v1",
            "Resources": {
              "Requests": { "cpu": "500m", "memory": "256Mi" },
              "Limits": { "cpu": "1000m", "memory": "512Mi" }
            }
          }
        ]
      },
      "Graph": {
        "Name": "input-transformer",
        "Type": "TRANSFORMER",
        "EndpointType": "REST",
        "Children": [
          {
            "Name": "fs33kafka",
            "Type": "MODEL",
            "Implementation": "MLFLOW_SERVER",
            "ModelUri": "s3://models/fs33kafka",
            "Children": []
          }
        ]
      }
    }
  ]
}